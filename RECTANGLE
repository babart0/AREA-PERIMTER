package meyn;

/**
 *
 * @author Student
 */
public class rectangle {
    
       public double length;
       public double width;
    
    public  rectangle (double length, double width){
    this.length = length; 
    this.width = width;
      
    }
    public double getArea (){
        return length * width;
        
    }
    
    public double getPerimeter (){
        
        return 2 * length + 2 * width;
    }
    
    
}
    
    
    
    
    
    
    
   
